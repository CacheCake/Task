<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="signIn" namespace="/" extends="task">
		<action name="signIn" class="action.SignInAction" method="DoSignIn">
			<result name="HRSignInSuccess">/HR/hr_main.jsp</result>
			<result name="MgrSignInSuccess">/Mgr/mgr_main.jsp</result>
			<result name="StfSignInSuccess">/HR/hr_main3.jsp</result>
			<result name="SignInFailed">/back_end_header.jsp</result>
		</action>
	</package>
</struts>